version 16
__lua__
anims = {
	walk = {
		{1, 2, 4},
		{33, 2, 4},
		{35, 2, 4},
		{33, 2, 4}
	},
	sword = {
		{1, 2, 30},
		{3, 2, 3},
		{5, 3, 1},
		{8, 2, 2},
		{3, 2, 1}
	}
}

frames = anims.walk
framenum = 1
frame = frames[1]
timer = frames[1][3]
function _update()
	if (timer <= 0) then
		framenum = (framenum + 1) % #frames
		frame = frames[framenum + 1]
		timer = frame[3]
	else
		timer -= 1
	end
end

function _draw()
	cls()
	spr(frame[1], 0,0, frame[2], 2)
end
